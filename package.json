{
  "name": "ah-django-unchained-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "test --watch",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js src/**/*.jsx --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "acorn": "^6.0.7",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.6.0",
    "react-scripts": "2.1.3",
    "watch": "^1.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "redux-immutable-state-invariant": "^2.1.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/src/serviceWorker.jsx",
      "!**/src/index.jsx",
      "!**/src/store/index.js",
      "!**/src/store/rootReducer.js"
    ]
  }
